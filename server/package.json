    "email": "mdy6099@gmail.com"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StyleShare/CodingTest-YoonSeongKyeong"
  },
  "bugs": {
    "url": "https://github.com/StyleShare/CodingTest-YoonSeongKyeong"
  },
  "tags": [
    "orm",
    "typescript",
    "typescript-orm",
    "typeorm-sample",
    "typeorm-example",
    "typeorm-express-example"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "build": "rm -rf dist && tsc",
    "start": "yarn build && cross-env NODE_ENV=production node dist/index.js",
    "dev": "yarn build && cross-env NODE_ENV=develop node --inspect -r ts-node/register src/index.ts",
    "test": "yarn build && cross-env NODE_ENV=test jest --runInBand --coverage --forceExit --detectOpenHandles --verbose",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./config/env/.env"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build"
    }
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql": "^2.15.0",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.11.1",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/morgan": "^1.7.37",
    "@types/node": "^9.6.5",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^25.1.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.3.3333"
  }
}
